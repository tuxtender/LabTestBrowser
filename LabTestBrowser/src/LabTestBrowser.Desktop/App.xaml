<Application x:Class="LabTestBrowser.Desktop.App"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:ui="http://schemas.modernwpf.com/2019"
			 xmlns:converters="clr-namespace:LabTestBrowser.Desktop.Converters">
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ui:ThemeResources />
				<ui:XamlControlsResources />
				<ResourceDictionary Source="Resources/Icons.xaml" />
			</ResourceDictionary.MergedDictionaries>
			<converters:AgeConverter x:Key="AgeConverter" />
			<converters:DateConverter x:Key="DateConverter" />
			<converters:NotificationIconConverter x:Key="NotificationIconConverter" />
			<converters:ApplicationThemeConverter x:Key="ApplicationThemeConverter" />
			<converters:DoubleToIntConverter x:Key="DoubleToIntConverter" />
			<converters:BooleanInverterConverter x:Key="BooleanInverterConverter" />
			<BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
		</ResourceDictionary>
	</Application.Resources>
</Application>